{
  "name": "grow-com-server-evaluation",
  "version": "1.0.0",
  "description": "Competency evaluation for Grow.com; building a basic Nodejs app using Express to connect to the swapi.co API's.",
  "engines": {
    "node": "5.5.0"
  },
  "ecmaFeatures": {
    "modules": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/derekclair/grow-com-server-evaluation.git"
  },
  "main": "server.js",
  "scripts": {
		"start": "node server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ."
  },
	"homepage": "https://grow-com-server.herokuapp.com",
  "author": "Derek Clair<derek@derekclair.com>",
  "license": "ISC",
  "dependencies": {
    "ejs": "^2.5.6",
    "express": "^4.15.0",
    "lodash.sortby": "^4.7.0",
    "request-promise": "^4.1.1",
    "swapi-node": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "nodemon": "^1.11.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "airbnb-base"
    ],
    "rules": {
      "spaced-comment": 0,
      "no-console": "warn"
    }
  },
  "private": true
}
